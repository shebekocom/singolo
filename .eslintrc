{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:prettier/recommended", "eslint-config-prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "strict": 0,
    "import/no-unresolved": "off",
    "import/extensions": "off"
  },
  "plugins": ["prettier"]
}
